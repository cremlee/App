<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cqube="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/spring_wood"
    android:scrollbars="none">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >
        <android.luna.ViewUi.widget.SettingItemTextView1
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview1Text="@string/SVR_PAY_ONLINE_TITLE"
            cqube:textview1TextBackground="@color/title_text_color2"
            cqube:textview1TextColor="@android:color/white" />
        <android.luna.ViewUi.widget.SettingItemTextView2
            android:id="@+id/net"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview2TextTitle="@string/SVR_PAY_ONLINE_NET"
            cqube:textview2TextValue="Online" />
        <android.luna.ViewUi.widget.SettingItemTextView2
            android:id="@+id/ip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview2TextTitle="@string/SVR_PAY_ONLINE_ADDRESS"
            cqube:textview2TextValue="172.24.60.251" />
        <android.luna.ViewUi.widget.SettingItemTextView1
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview1Text="@string/SVR_PAY_ONLINE_TITLE_ALI"
            cqube:textview1TextBackground="@color/title_text_color2"
            cqube:textview1TextColor="@android:color/white" />
        <android.luna.ViewUi.widget.SettingItemTextView2
            android:id="@+id/auth_ali_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview2TextTitle="@string/SVR_PAY_ONLINE_AUTH_INFO"
            cqube:textview2TextValue="-" />
        <android.luna.ViewUi.widget.SettingItemTextView2
            android:id="@+id/auth_ali_import"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview2TextTitle="@string/SVR_PAY_ONLINE_AUTH_IMPORT"
            cqube:textview2TextValue=">" />
        <android.luna.ViewUi.widget.SettingItemTextView1
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview1Text="@string/SVR_PAY_ONLINE_TITLE_WCT"
            cqube:textview1TextBackground="@color/title_text_color2"
            cqube:textview1TextColor="@android:color/white" />
        <android.luna.ViewUi.widget.SettingItemTextView2
            android:id="@+id/auth_wechat_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview2TextTitle="@string/SVR_PAY_ONLINE_AUTH_INFO"
            cqube:textview2TextValue="-" />
        <android.luna.ViewUi.widget.SettingItemTextView2
            android:id="@+id/auth_wechat_import"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            cqube:textview2TextTitle="@string/SVR_PAY_ONLINE_AUTH_IMPORT"
            cqube:textview2TextValue=">" />
    </LinearLayout>
</ScrollView>